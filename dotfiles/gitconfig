[user]
	email = vbauerster@gmail.com
	name = Vladimir Bauer
[core]
  excludesfile = ~/.gitignore
[alias]
  co = checkout
  ci = commit
  st = status
  b = branch -v
  br = branch -av
  d  = diff -w
  r = remote -v
  hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
	stat = log --pretty=format:\"[%h] %ae, %ar: %s\" --stat
  last = cat-file commit HEAD
